@article{jagherKowmOasisHuman2015,
  title = {El {{Kowm Oasis}}, Human Settlement in the {{Syrian Desert}} during the {{Pleistocene}}},
  author = {Jagher, Reto and Elsuede, Hani and Le Tensorer, Jean-Marie},
  year = {2015},
  month = nov,
  journal = {L'Anthropologie},
  volume = {119},
  number = {5},
  pages = {542--580},
  issn = {0003-5521},
  doi = {10.1016/j.anthro.2015.10.003},
  urldate = {2020-10-27},
  abstract = {The area of the El Kowm oasis in the centre of today's Syria is a unique showcase of Middle Eastern prehistory not only for its geographic position within the Arabian Desert, but also for the depth of its history. In fact the core area, about a dozen kilometres across, with the numerous natural springs were an important attractor for game and humans, visiting regularly this region since more than 1.8 million years. All over the periods, from the Lower Palaeolithic to the arrival of farmers, definitely arid conditions prevailed, perfectly showing human adaption to this particular environment, as has been clearly demonstrated by several extensive excavations of exceptionally rich Palaeolithic sites. Exceptional preservation conditions due to a particular geological setting in the context of active or dry springs offer an extraordinary resolution of human behaviour within a limited landscape during the complete period of the Pleistocene, illustrating cognitive capacities of early man to cope readily with arid environments, challenge the migration routes proposed by Out of Africa diffusion models, typically considered to be corridors with a superior environmental potential. R{\'e}sum{\'e} La r{\'e}gion de l'oasis d'El Kowm, dans le centre de la Syrie actuelle, est un exemple unique pour la Pr{\'e}-histoire du Proche-Orient. Ceci non seulement en raison de son emplacement dans le d{\'e}sert d'Arabie, mais aussi pour la dur{\'e}e de son histoire. En effet, {\`a} peine d'une douzaine de kilom{\`e}tres de diam{\`e}tre, la r{\'e}gion de l'oasis avec ses nombreuses sources naturelles {\'e}tait un point d'attraction pour les animaux et les humains qui sont revenus r{\'e}guli{\`e}rement dans cette r{\'e}gion depuis plus de 1,8~millions d'ann{\'e}es. Des conditions arides ont pr{\'e}domin{\'e} pendant toutes les p{\'e}riodes du Pal{\'e}olithique jusqu'{\`a} l'arriv{\'e}e des premiers paysans. Cela prouve parfaitement la capacit{\'e} d'adaptation des populations humaines {\`a} cet environnement bien particulier, ce que les fouilles extensives de sites Pal{\'e}olithiques particuli{\`e}rement riches ont d{\'e}montr{\'e}. Des conditions exceptionnelles de pr{\'e}servations dues {\`a} une configuration g{\'e}ologique particuli{\`e}re dans un contexte de sources actives ou temporairement ass{\'e}ch{\'e}es offrent une r{\'e}solution exceptionnelle du comportement humain au sein d'un paysage limit{\'e} pendant toute la dur{\'e}e du Pl{\'e}istoc{\`e}ne. Cela illustre les capacit{\'e}s cognitives des premiers humains s'adaptant ais{\'e}ment {\`a} des milieux fortement arides et remet en cause les voies de circulation des migrations <<~out of Africa~>>, traditionnellement associ{\'e}es {\`a} des couloirs avec un potentiel environnemental beaucoup plus favorable.},
  langid = {english},
  keywords = {Arabian Desert,Desert d'Arabie,Dynamique de peuplement,El Kowm,Levant,Palaeoecology,Palaeolithic,Paleoecologie,Paleolithique,Pleistocene,Settlement dynamic,Syria,Syrie},
  file = {/Users/shaigordin/Zotero/storage/IVE2TRI4/S0003552115000746.html}
}

