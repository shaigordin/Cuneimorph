  author = {Chiang, Yao-Yi and Duan, Weiwei and Leyk, Stefan and Uhl, Johannes H. and Knoblock, Craig A.},
  editor = {Chiang, Yao-Yi and Duan, Weiwei and Leyk, Stefan and Uhl, Johannes H. and Knoblock, Craig A.},
  year = {2020},
