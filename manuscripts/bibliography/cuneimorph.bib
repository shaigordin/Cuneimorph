@article{eliyahu-beharIronBronzeProduction2012,
  title = {Iron and Bronze Production in {{Iron Age IIA Philistia}}: New Evidence from {{Tell}} Es-{{Safi}}/{{Gath}}, {{Israel}}},
  shorttitle = {Iron and Bronze Production in {{Iron Age IIA Philistia}}},
  author = {{Eliyahu-Behar}, Adi and {Yahalom-Mack}, Naama and Shilstein, Sana and Zukerman, Alexander and {Shafer-Elliott}, Cynthia and Maeir, Aren M. and Boaretto, Elisabetta and Finkelstein, Israel and Weiner, Steve},
  year = {2012},
