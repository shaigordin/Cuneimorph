@article{rupkeReligionAlsUrbanitat2019,
  title = {Religion Als {{Urbanit{\"a}t}}: {{Ein}} Anderer {{Blick}} Auf {{Stadtreligion}}},
  shorttitle = {Religion Als {{Urbanit{\"a}t}}},
  author = {R{\"u}pke, J{\"o}rg},
  year = {2019},
  month = apr,
  journal = {Zeitschrift f{\"u}r Religionswissenschaft},
  volume = {27},
  number = {1},
  pages = {174--195},
  issn = {2194-508X, 0943-8610},
  doi = {10.1515/zfr-2018-0018},
  urldate = {2021-01-10},
  abstract = {Zusammenfassung             Religion in der Gro{\ss}stadt ist ein brennendes Thema, nicht nur f{\"u}r die Forschung. Der Blick auf die Stadt als Hort der S{\"a}kularisierung ist durch neue Formen religi{\"o}ser Pr{\"a}senz, Debatten um Religion als kulturelles Erbe, durch blutige Pogrome und Erfahrungen der ,,fundamentalistischen Stadt'' ersch{\"u}ttert worden. Der Blick auf Religion und Stadt in der Antike durch die Linse von Numa Fustel de Coulange kann einen unerwarteten Beitrag zur gegenw{\"a}rtige Debatte liefern: n{\"a}mlich dann, wenn man einen verengten Blick auf Polisreligion als Staatsreligion {\"u}berwindet. Der Aufsatz will die These plausibilisieren, dass religi{\"o}se Praktiken und Vorstellungen wichtig f{\"u}r die Stabilisierung und weitere Verbreitung von St{\"a}dten in der Antike waren und sich Religion und Urbanit{\"a}t als zwei Seiten einer Medaille entwickelt haben. In diesem Sinne wird eine Weiterentwicklung des Begriffs ,,urban religion'' als ,,urbanisierte Religion`` vorgeschlagen.}
}

