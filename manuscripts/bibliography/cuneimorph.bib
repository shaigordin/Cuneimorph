@article{nmnMs`ShyshqLrts1998,
  title = {{מסע שישק לארץ ישראל בראי הכתובות המצריות, המקרא והממצא הארכיאולוגי}},
  author = {נאמן, נדב},
  year = {1998},
  journal = {ציון},
  volume = {סג},
  number = {ג},
  eprint = {23563442},
  eprinttype = {jstor},
  pages = {247--276},
  publisher = {Historical Society of Israel / החברה ההיסטורית הישראלית},
  issn = {0044-4758},
  urldate = {2021-02-09},
  abstract = {The article contrasts the textual (biblical and extra-biblical) and archaeological evidence relating to Shishak's campaign to Palestine. The Egyptian sources are taken as a point of departure for reconstructing the course of the campaign and its objectives. The texts of 1 Kings 14:25---28 and 2 Chronicles 12:2---12 are examined separately, each in its own right. Shishak's campaign is the earliest episode that is related in both biblical and extra-biblical sources, and it is evident that the author of Kings had a source, possibly an old chronicle, from which he learned of the Egyptian campaign and tried to describe it in his history. The author of Chronicles, on the other hand, had no source other than the Book of Kings, and his 'logical' reworking and expansion of this source, coupled with his distinctive doctrine of retribution, led him far astray from historical reality. Discussion of the archaeological aspects of the campaign indicates that it had no long-range effect on sites located in the northern parts of Palestine, but was disastrous for the settlement in the Negev Highlands. The Kingdom of Israel gradually recovered and developed in the ninth century into a full blown state; the inhabitants of the Negev Highlands were unable to recover and reverted to their former mode of existence, pastoral nomadism, deserting their settlements and they became once again an 'invisible' element in the desert area.},
  langid = {hebrew}
}

