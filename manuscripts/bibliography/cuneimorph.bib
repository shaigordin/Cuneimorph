@article{gonzalez-quevedoUse3DPhotogrammetry2021,
  title = {The {{Use}} of {{3D Photogrammetry}} in the {{Analysis}}, {{Visualization}}, and {{Dissemination}} of the {{Indigenous Archaeological Heritage}} of the {{Greater Antilles}}},
  author = {{Gonz{\'a}lez-Quevedo}, Esteban Rub{\'e}n Grau and Godoy, Silvia Teresita Hern{\'a}ndez and Ortega, Racso Fern{\'a}ndez and Herrera, Ulises Miguel Gonz{\'a}lez and Dom{\'i}nguez, Jorge Garcell and Prada, Alexis Morales and Belando, Adolfo Jos{\'e} L{\'o}pez and Roksandic, Mirjana and de Armas, Yadira Chinique},
  year = {2021},
