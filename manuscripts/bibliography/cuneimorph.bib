@article{macklinFluvialRecordClimate2012,
  title = {The Fluvial Record of Climate Change},
  author = {Macklin, M. G. and Lewin, J. and Woodward, J. C.},
  year = {2012},
  month = may,
  journal = {Philosophical Transactions of the Royal Society A: Mathematical, Physical and Engineering Sciences},
  volume = {370},
  number = {1966},
  pages = {2143--2172},
  publisher = {Royal Society},
  doi = {10.1098/rsta.2011.0608},
  urldate = {2022-01-28},
  abstract = {Fluvial landforms and sediments can be used to reconstruct past hydrological conditions over different time scales once allowance has been made for tectonic, base-level and human complications. Field stratigraphic evidence is explored here at three time scales: the later Pleistocene, the Holocene, and the historical and instrumental period. New data from a range of field studies demonstrate that Croll--Milankovitch forcing, Dansgaard--Oeschger and Heinrich events, enhanced monsoon circulation, millennial- to centennial-scale climate variability within the Holocene (probably associated with solar forcing and deep ocean circulation) and flood-event variability in recent centuries can all be discerned in the fluvial record. Although very significant advances have been made in river system and climate change research in recent years, the potential of fluvial palaeohydrology has yet to be fully realized, to the detriment of climatology, public health, resource management and river engineering.},
  keywords = {climate change,floods,fluvial sediments,palaeohydrology,rivers},
  file = {/Users/shaigordin/Zotero/storage/BXB67II4/Macklin et al. - 2012 - The fluvial record of climate change.pdf}
}

