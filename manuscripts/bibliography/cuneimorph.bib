@article{vellaNewInsightsPrehispanic2019,
  title = {New Insights into Prehispanic Urban Organization at {{Tiwanaku}} ({{NE Bolivia}}): {{Cross}} Combined Approach of Photogrammetry, Magnetic Surveys and Previous Archaeological Excavations},
  shorttitle = {New Insights into Prehispanic Urban Organization at {{Tiwanaku}} ({{NE Bolivia}})},
  author = {Vella, M. -A. and Ernenwein, E. G. and Janusek, J. W. and Koons, M. and Thiesson, J. and Sanchez, C. and Gu{\'e}rin, R. and Camerlynck, C.},
  year = {2019},
  month = feb,
  journal = {Journal of Archaeological Science: Reports},
  volume = {23},
  pages = {464--477},
  issn = {2352-409X},
  doi = {10.1016/j.jasrep.2018.09.023},
  urldate = {2023-04-10},
  abstract = {The prehispanic site of Tiwanaku, located in northeastern Bolivia, was the focus of many studies during the past few decades. However, much of the site remains unexplored, leaving many questions unanswered about the location of dense archaeological deposits, the nature of urban organization, and water management strategies---specifically those located in the eastern sector of the Akapana Pyramid. Orthophoto mosaics and Digital Elevation Models derived from drone imagery helped identify archaeological features and anthropogenic mounds. New magnetic survey produced with a cesium gradiometer was merged with previous surveys (fluxgate and cesium gradiometer). The integration of maps and plans from six areas of a previous archaeological investigation within a common Geographical Information System helped relate geophysical anomalies to archaeological features. Our results demonstrate a high level of urban organization associating monumental buildings to open ritual spaces and to densely populated areas during Tiwanaku IV (500--800\,CE) and V (800--1100\,CE). The complexity of the urban organization is also demonstrated by landscape modifications such as a complex water management system and at least three terraces that augmented the monumentality of the Akapana Pyramid This interdisciplinary approach, innovative in Bolivia, provides new insight into one of the most significant archaeological sites in the Andes.},
  langid = {english},
  keywords = {Andes,Archaeological excavations,Bolivia,Magnetic surveys,Photogrammetry,Tiwanaku},
  file = {/Users/shaigordin/Zotero/storage/IQ3W776G/Vella et al. - 2019 - New insights into prehispanic urban organization a.pdf;/Users/shaigordin/Zotero/storage/NTJRZNA2/S2352409X18302943.html}
