@article{dennisonInstitutionsDemographyEconomic2016,
  title = {Institutions, {{Demography}}, and {{Economic Growth}}},
  author = {Dennison, Tracy K. and Ogilvie, Sheilagh},
  year = {2016},
  month = mar,
  journal = {The Journal of Economic History},
  volume = {76},
  number = {1},
  pages = {205--217},
  publisher = {Cambridge University Press},
  issn = {0022-0507, 1471-6372},
  doi = {10.1017/S0022050716000486},
  urldate = {2021-10-29},
  abstract = {This article evaluates criticisms by Sarah G. Carmichael, Alexandra de Pleijt, Jan Luiten van Zanden, and Tine De Moor of our view of the European Marriage Pattern (EMP), and explains why their claims are incorrect. We elaborate our arguments concerning the institutional sources of economic growth, explore the relationship between women's position and the EMP, analyze the two-way links between demographic and economic behavior, and explicate aspects of our empirical analysis that these scholars find puzzling. The causes of European economic growth, we reiterate, are not to be found in the EMP but rather must be sought in the wider framework of nonfamilial institutions.},
