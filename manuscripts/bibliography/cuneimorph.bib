@book{finckeAncientMesopotamianHerbal2021,
  title = {An {{Ancient Mesopotamian Herbal Handbook}}: {{The Series URU}}.{{AN}}.{{NA}} and {{M{\'U}D-UR}}.{{MAḪ}}. {{Volume}} 1: {{The Tablets}}},
  author = {Fincke, Jeanette C.},
