@article{flourisAnalysisGreekPrehistoric2024,
  title = {Analysis of {{Greek}} Prehistoric Combat in Full Body Armour Based on Physiological Principles: {{A}} Series of Studies Using Thematic Analysis, Human Experiments, and Numerical Simulations},
  shorttitle = {Analysis of {{Greek}} Prehistoric Combat in Full Body Armour Based on Physiological Principles},
  author = {Flouris, Andreas D. and Petmezas, Stavros B. and Asimoglou, Panagiotis I. and Vale, Jo{\~a}o P. and Mayor, Tiago S. and Giakas, Giannis and Jamurtas, Athanasios Z. and Koutedakis, Yiannis and Wardle, Ken and Wardle, Diana},
  year = {2024},
