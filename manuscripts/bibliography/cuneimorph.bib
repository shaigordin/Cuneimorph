@article{wertherWayFluvialAnthroposphere2021,
  title = {On the {{Way}} to the {{Fluvial Anthroposphere}}---{{Current Limitations}} and {{Perspectives}} of {{Multidisciplinary Research}}},
  author = {Werther, Lukas and Mehler, Natascha and Schenk, Gerrit Jasper and Zielhofer, Christoph},
